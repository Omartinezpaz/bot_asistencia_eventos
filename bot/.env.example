# Configuración del bot de Telegram
BOT_TOKEN=tu_token_del_bot

# ID de Telegram del administrador (para comandos especiales)
ADMIN_TELEGRAM_ID=tu_id_de_telegram

# Configuración de la base de datos
DATABASE_URL=postgres://usuario:contraseña@localhost:5432/nombre_base_datos

# API Keys para proveedores de geocodificación
GOOGLE_MAPS_API_KEY=tu_api_key_de_google
MAPBOX_API_KEY=tu_api_key_de_mapbox
OPENSTREETMAP_API_KEY=tu_api_key_de_openstreetmap

# Configuración del servidor
PORT=3000
NODE_ENV=development

# URL para el webhook (solo para producción)
WEBHOOK_URL=https://tu-dominio.com

# Nombre de usuario del bot de Telegram (sin @)
BOT_USERNAME=notif_eventos_bot

# Configuración del panel de administración
JWT_SECRET=secreto-jwt-para-panel-admin
ADMIN_PASSWORD=contraseña-para-panel-admin 